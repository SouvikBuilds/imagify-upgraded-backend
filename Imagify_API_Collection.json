{
  "info": {
    "name": "Imagify API Collection",
    "description": "Complete API collection for Imagify application",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8000/api/v2"
    },
    {
      "key": "accessToken",
      "value": ""
    }
  ],
  "item": [
    {
      "name": "User Management",
      "item": [
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"fullName\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/users/register",
              "host": ["{{baseUrl}}"],
              "path": ["users", "register"]
            }
          }
        },
        {
          "name": "Login User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"john@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/users/login",
              "host": ["{{baseUrl}}"],
              "path": ["users", "login"]
            }
          }
        },
        {
          "name": "Get Current User",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/users/current-user",
              "host": ["{{baseUrl}}"],
              "path": ["users", "current-user"]
            }
          }
        },
        {
          "name": "Get User Credits",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/users/credit",
              "host": ["{{baseUrl}}"],
              "path": ["users", "credit"]
            }
          }
        },
        {
          "name": "Logout User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/users/logout",
              "host": ["{{baseUrl}}"],
              "path": ["users", "logout"]
            }
          }
        },
        {
          "name": "Refresh Access Token",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"refreshToken\": \"your_refresh_token_here\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/users/refresh-token",
              "host": ["{{baseUrl}}"],
              "path": ["users", "refresh-token"]
            }
          }
        }
      ]
    },
    {
      "name": "Image Processing",
      "item": [
        {
          "name": "Generate Image from Text",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"prompt\": \"A beautiful sunset over mountains\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/images/generate",
              "host": ["{{baseUrl}}"],
              "path": ["images", "generate"]
            }
          }
        },
        {
          "name": "Remove Background",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "image",
                  "type": "file",
                  "src": ""
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/images/remove-bg",
              "host": ["{{baseUrl}}"],
              "path": ["images", "remove-bg"]
            }
          }
        },
        {
          "name": "Upscale Image",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "image",
                  "type": "file",
                  "src": ""
                },
                {
                  "key": "targetWidth",
                  "value": "1920",
                  "type": "text"
                },
                {
                  "key": "targetHeight",
                  "value": "1080",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/images/upscale",
              "host": ["{{baseUrl}}"],
              "path": ["images", "upscale"]
            }
          }
        },
        {
          "name": "Uncrop Image",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "image",
                  "type": "file",
                  "src": ""
                },
                {
                  "key": "extendLeft",
                  "value": "100",
                  "type": "text"
                },
                {
                  "key": "extendDown",
                  "value": "100",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/images/uncrop",
              "host": ["{{baseUrl}}"],
              "path": ["images", "uncrop"]
            }
          }
        },
        {
          "name": "Remove Text from Image",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "image",
                  "type": "file",
                  "src": ""
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/images/remove-text",
              "host": ["{{baseUrl}}"],
              "path": ["images", "remove-text"]
            }
          }
        },
        {
          "name": "Replace Background",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "image",
                  "type": "file",
                  "src": ""
                },
                {
                  "key": "prompt",
                  "value": "beach sunset background",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/images/replace-bg",
              "host": ["{{baseUrl}}"],
              "path": ["images", "replace-bg"]
            }
          }
        }
      ]
    }
  ]
}